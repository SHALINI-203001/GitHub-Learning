#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int arr[1000],n,i,sum=0;

scanf("%d\n",&n);
//printf("\n");
for(i=1;i<=n;i++)
{   
    scanf("%d", &arr[i]);
  // printf("%d ",arr[i]);
   sum=sum+arr[i];
}
// printf("\n");
    printf("%d",sum);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

